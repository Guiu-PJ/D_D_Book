<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".usuarios_admin">

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/pagina_libro"
    tools:context=".MainActivity">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton"
        android:layout_width="73dp"
        android:layout_height="61dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="32dp"
        android:clickable="true"
        android:contentDescription="@string/flecha"
        android:src="@drawable/return_arrow"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:focusable="true" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="102dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageButton
                android:id="@+id/img_perfil_humpty"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="50dp"
                android:adjustViewBounds="false"
                android:background="?selectableItemBackgroundBorderless"
                android:contentDescription="usuario"
                android:scaleType="fitCenter"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="MissingConstraints,DuplicateSpeakableTextCheck"
                tools:srcCompat="@drawable/imagen_usuario" />

            <TextView
                android:id="@+id/txt_nombre_humpty"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="58dp"
                android:text="@string/humpty_dumpty"
                android:textSize="20sp"
                app:layout_constraintStart_toEndOf="@+id/img_perfil_humpty"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/txt_correo_humpty"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="8dp"
                android:text="@string/dumpty_humpty_copernic_cat"
                android:textSize="16sp"
                app:layout_constraintStart_toEndOf="@+id/img_perfil_humpty"
                app:layout_constraintTop_toBottomOf="@+id/txt_nombre_humpty" />

            <ImageView
                android:id="@+id/img_ajustes_humpty"
                android:layout_width="100dp"
                android:layout_height="95dp"
                android:layout_marginTop="34dp"
                android:contentDescription="@string/ajustes"
                android:src="@drawable/tuerca"
                app:layout_constraintStart_toEndOf="@+id/txt_correo_humpty"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/img_perfil_jorge"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="17dp"
                android:contentDescription="usuariojorge"
                android:src="@drawable/jorge"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/img_perfil_humpty" />

            <TextView
                android:id="@+id/txt_nombre_jorge"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="33dp"
                android:text="@string/jorge_cerdo"
                android:textSize="20sp"
                app:layout_constraintStart_toEndOf="@+id/img_perfil_jorge"
                app:layout_constraintTop_toBottomOf="@+id/txt_correo_humpty" />

            <TextView
                android:id="@+id/txt_nombre_calamardo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="33dp"
                android:text="@string/ricardo_calamar"
                android:textSize="20sp"
                app:layout_constraintStart_toEndOf="@+id/im_perfil_calamardo"
                app:layout_constraintTop_toBottomOf="@+id/txt_correo_jorge" />

            <TextView
                android:id="@+id/txt_correo_jorge"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:text="@string/cerdo_jorge_copernic_cat"
                android:textSize="16sp"
                app:layout_constraintStart_toEndOf="@+id/img_perfil_jorge"
                app:layout_constraintTop_toBottomOf="@+id/txt_nombre_jorge" />

            <TextView
                android:id="@+id/txt_correo_calamardo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:text="@string/calamar_ricardo_copernic_cat"
                android:textSize="16sp"
                app:layout_constraintStart_toEndOf="@+id/im_perfil_calamardo"
                app:layout_constraintTop_toBottomOf="@+id/txt_nombre_calamardo" />

            <ImageView
                android:id="@+id/img_ajustes_jorge"
                android:layout_width="100dp"
                android:layout_height="95dp"
                android:layout_marginStart="31dp"
                android:contentDescription="@string/ajustes"
                android:src="@drawable/tuerca"
                app:layout_constraintStart_toEndOf="@+id/txt_correo_jorge"
                app:layout_constraintTop_toBottomOf="@+id/img_ajustes_humpty" />

            <ImageView
                android:id="@+id/im_perfil_calamardo"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="17dp"
                android:contentDescription="usuariocalamardo"
                android:src="@drawable/calamardo"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/img_perfil_jorge" />

            <ImageView
                android:id="@+id/im_ajustes_calamardo"
                android:layout_width="100dp"
                android:layout_height="95dp"
                android:contentDescription="@string/ajustes"
                android:src="@drawable/tuerca"
                app:layout_constraintStart_toEndOf="@+id/txt_correo_calamardo"
                app:layout_constraintTop_toBottomOf="@+id/img_ajustes_jorge" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>